{
  "name": "@proofgeist/fmdapi",
  "version": "3.3.9",
  "description": "FileMaker Data API client",
  "main": "dist/index.js",
  "repository": "git@github.com:proofgeist/fm-dapi.git",
  "author": "Eric <37158449+eluce2@users.noreply.github.com>",
  "license": "MIT",
  "private": false,
  "type": "module",
  "types": "dist/index.d.ts",
  "bin": {
    "codegen": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/wv": {
      "import": "./dist/wv.js",
      "types": "./dist/wv.d.ts"
    },
    "./dist/tokenStore/*.js": "./dist/tokenStore/*.js",
    "./dist/tokenStore/*": "./dist/tokenStore/*"
  },
  "scripts": {
    "prepublishOnly": "tsc",
    "build": "tsc",
    "test": "jest",
    "changeset": "changeset",
    "release": "pnpm build && changeset publish --access public"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.1",
    "chalk": "4.1.2",
    "commander": "^9.5.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "ts-toolbelt": "^9.6.0"
  },
  "peerDependencies": {
    "zod": ">=3.21.4"
  },
  "devDependencies": {
    "@jest/types": "28.1.3",
    "@proofgeist/fm-webviewer-fetch": "^2.1.0",
    "@types/fs-extra": "^11.0.3",
    "@types/jest": "^28.1.8",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@upstash/redis": "^1.24.2",
    "eslint": "^8.52.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^28.1.3",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "zod": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "src",
    "dist",
    "dist-browser",
    "tokenStore",
    "utils",
    "stubs"
  ],
  "keywords": [
    "filemaker",
    "fms",
    "data api",
    "dapi",
    "fmrest",
    "fmdapi",
    "proofgeist",
    "fm-dapi"
  ]
}
