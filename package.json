    "dotenv": "^16.4.5",
    "fs-extra": "^11.1.1",
    "ts-toolbelt": "^9.6.0"
  },
  "peerDependencies": {
    "zod": ">=3.21.4"
  },
  "devDependencies": {
    "@jest/types": "28.1.3",
    "@proofgeist/fm-webviewer-fetch": "^2.1.0",
    "@types/fs-extra": "^11.0.3",
    "@types/jest": "^28.1.8",
    "@types/node": "^22.5.2",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@upstash/redis": "^1.31.6",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
